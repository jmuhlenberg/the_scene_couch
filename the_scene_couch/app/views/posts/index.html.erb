<div class="content">
  <h1 class='title'>the scene couch</h1>
  <h1>ALL POSTS</h1>
  <div class="all-posts">
    <% if @current_user %>
      <a href="/users/<%=@current_user.id%>/posts/new">CREATE A NEW POST</a>
    <% end %>

    <% @posts.each do |post| %>
      <div class="card">
        <%= link_to post.title, post %>
        <%= post.video %>
        <%= post.content %>
      </div>
    <% end %>
  </div>
</div>
